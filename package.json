{
  "name": "houndrace-contracts",
  "version": "1.0.0",
  "description": "",
  "main": "index.js",
  "scripts": {
    "test": "npx hardhat test workflows/full-workflow.ts",
    "deployment-workflow-test": "npx hardhat test workflows/deployment-workflow.ts",
    "full-deployment-workflow-test": "TOTAL_RACES=10 npx hardhat test workflows/full-deployment-workflow.ts",
    "core-workflow-test": "npx hardhat test workflows/core-workflow.ts",
    "full-workflow-test": "npx hardhat test workflows/full-workflow.ts",
    "races-statistics-generation-test": "TOTAL_RACES=20 INITIAL_HOUNDS_NUMBER=12 npx hardhat test workflows/races-generator.ts",
    "typegenall": "cp -r typechain-types/* $TYPEGEN_ALL_PATH",
    "compile": "rm -rf cache && rm -rf artifacts && rm -rf typechain-types && npx hardhat compile",
    "safe-hardhat-test": "echo 'Recommended for node > 16.x' && export SET NODE_OPTIONS=--openssl-legacy-provider && hardhat test",
    "deployments-to-env": "cd plugins && ts-node deployments-to-env.ts",
    "deployments-to-yml": "cd plugins && ts-node deployments-to-yml.ts",
    "deploy-rinkeby": "hardhat run scripts/deploy.ts --network rinkeby",
    "deploy-polygonMumbai": "hardhat run scripts/deploy.ts --network polygonMumbai",
    "deploy-polygon": "hardhat run scripts/deploy.ts --network polygon",
    "deploy-lootboxes-polygonMumbai": "HOUNDS=0x8d6E14AC7e279221c2060AF9791aF78859f266cd PAYMENTS=0x64565db658D89db255af710E21d0f4acC1Ca4f45 hardhat run scripts/utils/deploy-lootboxes-ecosystem.ts --network polygonMumbai",
    "deploy": "hardhat run scripts/deploy.ts",
    "deploy-lootboxes-polygon": "HOUNDS=0xd3E9983bE90986e0e772733592fE82fE31497070 PAYMENTS=0x34bdFa5AADc1A48ce158104c9ee42578A8ed7C45 hardhat run scripts/utils/deploy-lootboxes-ecosystem.ts --network polygon"
  },
