    "full-deployment-workflow-test": "TOTAL_RACES=10 npx hardhat test workflows/full-deployment-workflow.ts",
    "core-workflow-test": "npx hardhat test workflows/core-workflow.ts",
    "full-workflow-test": "npx hardhat test workflows/full-workflow.ts",
    "races-statistics-generation-test": "TOTAL_RACES=10 npx hardhat test workflows/races-generator.ts",
    "typegenall": "cp -r typechain-types/* $TYPEGEN_ALL_PATH",
    "compile": "npx hardhat compile",
    "safe-hardhat-test": "echo 'Recommended for node > 16.x' && export SET NODE_OPTIONS=--openssl-legacy-provider && hardhat test",
    "deployments-to-env": "cd plugins && ts-node deployments-to-env.ts",
    "deployments-to-yml": "cd plugins && ts-node deployments-to-yml.ts",
    "deploy-rinkeby": "hardhat run scripts/deploy.ts --network rinkeby",
    "deploy-polygonMumbai": "hardhat run scripts/deploy.ts --network polygonMumbai",
    "deploy": "hardhat run scripts/deploy.ts"
